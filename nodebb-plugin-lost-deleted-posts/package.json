{
  "name": "nodebb-plugin-lost-deleted-posts",
  "version": "0.3.1",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bharatrathi27412/nodebb-plugin-lost-deleted-posts"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "lost-deleted-posts",
    "shell"
  ],
  "husky": {
